{
    "name": "netgen/my-new-favorite-bundle",
    "description": "This holds description for my new favorite bundle that I'm working on",
    "type": "ezplatform-bundle",
    "keywords": [
        "ezpublish",
        "ezplatform",
        "my-favorite-library",
        "netgen",
        "my-favorite-bundle"
    ],
    "homepage": "https://github.com/netgen/MyNewFavoriteBundle",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Netgen",
            "homepage": "http://www.netgenlabs.com"
        }
    ],
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "matthiasnoback/symfony-config-test": "~4.0",
        "matthiasnoback/symfony-dependency-injection-test": "~3.0",
        "friendsofphp/php-cs-fixer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Netgen\\Bundle\\MyNewFavoriteBundle\\": "bundle",
            "Netgen\\MyNewFavoriteLib\\": "lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netgen\\Bundle\\MyNewFavoriteBundle\\Tests\\": "tests/bundle",
            "Netgen\\MyNewFavoriteLib\\Tests\\": "tests/lib"
        }
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit --colors=always",
        "fix": "@php vendor/bin/php-cs-fixer fix",
        "coverage": "@php -dzend_extension=xdebug.so vendor/bin/phpunit --colors=always"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
